{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": {
			"identifier": "pb:parachute",
			"materials": {
				"default": "entity_alphatest",
				"enchanted": "entity_alphatest_glint"
			},
			"textures": {
				"default": "textures/entity/parachute",
				"enchanted": "textures/misc/enchanted_item_glint"
			},
			"geometry": {
				"default": "geometry.parachute"
			},
			"scripts": {
				"pre_animation": [
					"v.main_hand = c.item_slot == 'main_hand';",
					"v.off_hand = c.item_slot == 'off_hand';",
					"v.head = c.item_slot == 'head';"
				],
				"animate": [
					{
						"firstperson_main_hand": "v.main_hand && c.is_first_person"
					},
					{
						"thirdperson_main_hand": "v.main_hand && !c.is_first_person"
					},
					{
						"firstperson_head": "v.head && c.is_first_person"
					},
					{
						"thirdperson_head": "v.head && !c.is_first_person"
					}
				]
			},
			"animations": {
				"thirdperson_main_hand": "animation.chute.bag",
				"firstperson_main_hand": "animation.chute.bag",
				"firstperson_head": "animation.chute.open",
				"thirdperson_head": "animation.chute.open"
			},
			"render_controllers": [
				"controller.render.item_default"
			]
		}
	}
}