{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "pb:small_cloud",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/small_cloud"
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": false,
				"velocity": true
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 200,
				"max_particles": 200
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 0.1
			},
			"minecraft:emitter_shape_point": {
				"offset": ["math.random(-0.,0.1)", 0, "math.random(-0.1,0.1)"],
				"direction": ["math.random(-1,1)", 0, "math.random(-1,1)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.cos(variable.emitter_age/5)*45*3"
			},
			"minecraft:particle_initial_speed": 1,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [1, 4, 1],
				"linear_drag_coefficient": -0.5
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.2, 0.2],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_age",
					"gradient": {
						"0.0": "#FFFFFFFF",
						"0.17": "#FFB6E6FF",
						"0.57": "#FF3094DA",
						"1.0": "#003554B1"
					}
				}
			}
		}
	}
}