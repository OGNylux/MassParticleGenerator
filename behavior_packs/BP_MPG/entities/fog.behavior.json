{
    "format_version": "1.20.0",
    "minecraft:entity": {
        "description": {
            "identifier": "pb:fog",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "pb:show_box": {
                "minecraft:timer": {
                    "time": 9,
                    "looping": true,
                    "time_down_event": {
                        "event": "evt:draw_hitbox",
                        "target": "self"
                    }
                }
            },
            "pb:show_radius": {
                "minecraft:timer": {
                    "time": 9,
                    "looping": true,
                    "time_down_event": {
                        "event": "evt:draw_outline",
                        "target": "self"
                    }
                }
            }
        },
        "components": {
            "minecraft:health": {
                "value": 1,
                "max": 1
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:scale": {
                "value": 1.0
            },
            "minecraft:persistent": {},
            "minecraft:collision_box": {
                "width": 0.01,
                "height": 0.01
            }
        },
        "events": {
            "evt:show_box": {
                "queue_command": {
                    "command": [ "scriptevent pb:drawCube 0.5" ]
                },
                "add": {
                    "component_groups": [
                        "pb:show_box"
                    ]
                }
            },
            "evt:hide_box": {
                "remove": {
                    "component_groups": [
                        "pb:show_box"
                    ]
                }
            },
            "evt:show_radius": {
                "queue_command": {
                    "command": [ "scriptevent pb:drawCube 16" ]
                },
                "add": {
                    "component_groups": [
                        "pb:show_radius"
                    ]
                }
            },
            "evt:hide_radius": {
                "remove": {
                    "component_groups": [
                        "pb:show_radius"
                    ]
                }
            },
            "evt:draw_outline": {
                "queue_command": {
                    "command": [ "scriptevent pb:drawCube 16" ]
                }
            },
            "evt:draw_hitbox": {
                "queue_command": {
                    "command": [ "scriptevent pb:drawCube 0.5" ]
                }
            }
        }
    }
}