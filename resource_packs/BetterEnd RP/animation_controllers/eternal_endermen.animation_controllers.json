{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.eternal_endermen": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["idle"],
                    "transitions": [
                        {
                            "walk": "q.ground_speed >= 0.7"
                        },
                        {
                            "attack": "query.skin_id == 1"
                        }
                    ],
                    "blend_transition": 0.2
                },
                "walk": {
                    "animations": [
                        "walk"
                    ],
                    "transitions": [
                        {
                            "idle": "q.ground_speed < 0.7"
                        },
                        {
                            "attack": "query.skin_id == 1"
                        }
                    ],
                    "blend_transition": 0.2
                },
                "attack": {
                    "animations": [
                        "attack"
                    ],
                    "transitions": [
                        {
                            "idle": "query.all_animations_finished"
                        }
                    ],
                    "blend_transition": 0.2
                }
            }
        }
    }
}