{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "pb:abyss_fog_main",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/fog_orange"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.size = 3;variable.lifetime = 4;variable.size_variant = 6;variable.age_growth = 0.2;variable.wobble_percent = 0;variable.spawn_radius = 32;variable.spawn_height = 16;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 25,
				"max_particles": 1000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_box": {
				"offset": [0, "variable.spawn_height", 0],
				"half_dimensions": ["variable.spawn_radius", "variable.spawn_height", "variable.spawn_radius"],
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "variable.lifetime"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "(variable.particle_random_3-0.5)*30",
				"rotation_rate": "(variable.particle_random_1-0.5)*30"
			},
			"minecraft:particle_initial_speed": 0,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["variable.particle_random_3-0.5", 1, "variable.particle_random_2-0.5"],
				"linear_drag_coefficient": 2,
				"rotation_drag_coefficient": 0.5
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(variable.size+(variable.particle_random_3*variable.size_variant))*(0.8+(math.cos(variable.particle_age*240)*variable.wobble_percent))", "(variable.size+(variable.particle_age*(variable.age_growth*2))+(variable.particle_random_3*variable.size_variant))*(0.8+(math.cos(variable.particle_age*240)*variable.wobble_percent))"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 1,
				"collision_radius": 0.25
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#00939393",
						"0.23": "#FCFFFFFF",
						"0.76": "#D4FFFFFF",
						"1.0": "#00939393"
					}
				}
			}
		}
	}
}