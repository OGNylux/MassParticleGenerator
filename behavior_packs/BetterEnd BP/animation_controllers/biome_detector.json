		"controller.animation.biome_detector_ambient": {
			"states": {
				"default": {
					"transitions": [
						{
							"ambient_abyss": "query.skin_id == 1"
						},
						{
							"ambient_void": "query.skin_id == 2"
						},
						{
							"ambient_ender": "query.skin_id == 3"
						},
						{
