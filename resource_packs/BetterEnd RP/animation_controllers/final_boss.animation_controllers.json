{
  "format_version": "1.10.0",
  "animation_controllers": {
      "controller.animation.dragon": {
          "initial_state": "default",
          "states": {
              "default": {
                  "transitions": [
                      {
                          "takeOff": "!query.is_on_ground"
                      },
                      {
                          "landing": "query.is_on_ground"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "attackGround": {
                  "animations": [
                      "attackGround"
                  ],
                  "particle_effects": [
                      {
                          "effect": "particle",
                          "locator": "locator1"
                      }
                  ],
                  "transitions": [
                      {
                          "idleStand": "query.any_animation_finished"
                      },
                      {
                          "attackFly": "!query.is_saddled && query.is_charged"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "attackFly": {
                  "animations": [
                      "attackFly",
                      "fly2"
                  ],
                  "particle_effects": [
                      {
                          "effect": "particle",
                          "locator": "locator1"
                      }
                  ],
                  "transitions": [
                      {
                          "attackGround": "query.is_delayed_attacking && query.is_saddled"
                      },
                      {
                          "idleFly": "query.any_animation_finished"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "idleFly": {
                  "animations": [
                      "fly",
                      "fly2"
                  ],
                  "transitions": [
                      {
                          "fly": "query.modified_move_speed > 0.1 && !query.is_on_ground && !query.has_rider"
                      },
                      {
                          "attackFly": "!query.is_saddled && query.is_charged"
                      },
                      {
                          "landing": "query.is_on_ground"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "idleStand": {
                  "animations": [
                      "idleStand"
                  ],
                  "transitions": [
                      {
                          "attackGround": "query.is_delayed_attacking && query.is_saddled"
                      },
                      {
                          "walk": "query.modified_move_speed > 0.1 && query.is_on_ground"
                      },
                      {
                          "takeOff": "!query.is_on_ground"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "fly": {
                  "animations": [
                      "fly",
                      "fly2"
                  ],
                  "transitions": [
                      {
                          "landing": "query.is_on_ground"
                      },
                      {
                          "attackFly": "!query.is_saddled && query.is_charged"
                      },
                      {
                          "idleFly": "query.modified_move_speed < 0.1 && !query.is_on_ground"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "walk": {
                  "animations": [
                      "walk"
                  ],
                  "transitions": [
                      {
                          "attackGround": "query.is_delayed_attacking && query.is_saddled"
                      },
                      {
                          "idleStand": "query.modified_move_speed < 0.1 && query.is_on_ground"
                      },
                      {
                          "takeOff": "!query.is_on_ground"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "takeOff": {
                  "animations": [
                      "takeOff"
                  ],
                  "transitions": [
                      {
                          "idleFly": "q.any_animation_finished"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              },
              "landing": {
                  "animations": [
                      "landing"
                  ],
                  "transitions": [
                      {
                          "idleStand": "q.any_animation_finished"
                      }
                  ],
                  "blend_transition": 0.2,
                  "blend_via_shortest_path": true
              }
          }
      }
  }
}