{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.player_bag": {
			"states": {
				"default": {
					"transitions": [
						{
							"vis": "query.is_item_name_any('slot.weapon.mainhand', 'pb:bag') && q.is_using_item"
						}
					]
				},
				"vis": {
					"transitions": [
						{
							"default": "!q.is_using_item"
						}
					],
					"on_entry": [
						"/replaceitem entity @s slot.weapon.mainhand 0 minecraft:air",
						"/tp @e[type=pb:bag,c=1] ~ ~ ~",
						"/event entity @e[type=pb:bag,c=1] pb:vis"
					]
				}
			}
		}
	}
}