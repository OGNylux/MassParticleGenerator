					"transitions": [
						{
							"default": "query.skin_id != 1"
						},
						{
							"ambient_void": "query.skin_id == 2"
						},
						{
							"ambient_ender": "query.skin_id == 3"
						},
						{
							"ambient_crystal": "query.skin_id == 4"
						}
					],
					"on_entry": [
						"/say abyss",
						"/fog @a[r=30] remove void_fog",
						"/fog @a[r=30] remove ender_fog",
						"/fog @a[r=30] remove crystal_fog",
                        "/fog @a[r=30] push pb:abyss_fog abyss_fog"
					]
				},
				"ambient_void": {
					"transitions": [
