{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "pb:dragon_breath",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/particle/darkness_explosion"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.size = 0.5;variable.age_growth = 0.8;variable.lifetime = 1.8;variable.size_variant = 0.2;variable.angle = 1.2;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 50,
				"max_particles": 400
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.5
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 0.3,
				"direction": ["(variable.particle_random_2-0.5)*variable.angle", 0, -1]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.random(variable.lifetime, variable.lifetime*1.3)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "(variable.particle_random_3-0.5)*360",
				"rotation_rate": "(variable.particle_random_1-0.5)*90"
			},
			"minecraft:particle_initial_speed": "Math.random(12,16)",
			"minecraft:particle_motion_dynamic": {
				"linear_drag_coefficient": 2
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.size+(variable.particle_random_1*variable.size_variant)+math.clamp(variable.particle_age*variable.age_growth, 0, 4)", "variable.size+(variable.particle_random_1*variable.size_variant)+math.clamp(variable.particle_age*variable.age_growth, 0, 4)"],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 64,
					"texture_height": 384,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [64, 64],
						"step_UV": [0, 64],
						"frames_per_second": 8,
						"max_frame": 6,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.1,
				"collision_radius": 0.1
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_age",
					"gradient": {
						"0.0": "#DBFFFFFF",
						"0.41": "#C4EE4EF4",
						"1.21": "#B0707070",
						"1.8": "#211A1A1A"
					}
				}
			}
		}
	}
}