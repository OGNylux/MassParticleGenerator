{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.boss_altar": {
      "states": {
        "default": {
          "transitions": [
            {
              "spawn_boss": "query.mark_variant == 7"
            }
          ]
        },
        "spawn_boss": {
          "on_entry": [
            "/scoreboard players set @a final_fight 0"
          ],
          "on_exit": []
        }
      }
    },
    "controller.animation.place_altar": {
      "states": {
        "default": {
          "transitions": [
            {
              "spawn_boss": "query.skin_id == 0"
            }
          ]
        },
        "spawn_boss": {
          "on_entry": [
            "/function place"
          ],
          "on_exit": []
        }
      }
    }
  }
}